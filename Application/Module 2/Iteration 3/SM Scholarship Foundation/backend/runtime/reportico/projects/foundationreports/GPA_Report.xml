subject_scholar_scholar_id Scholar_ID,
(
	SELECT n.scholar_last_name
	FROM scholar n
	WHERE n.scholar_id = subject_scholar_scholar_id
) Scholar_Last_Name,
(
	SELECT n.scholar_first_name
	FROM scholar n
	WHERE n.scholar_id = grade.subject_scholar_scholar_id
) Scholar_First_Name,
(
	SELECT s.school_name
	FROM school s
	WHERE s.school_id = grade.subject_scholar_school_school_id
) School_Name,
grade_school_year_start School_Year_Start,
grade_school_year_end School_Year_End,
(
	SELECT sub.subject_term
	FROM subject sub
	WHERE sub.subject_id = grade.subject_subject_id
) Term,
(
	SELECT sub.subject_name
	FROM subject sub
	WHERE sub.subject_id = grade.subject_subject_id
) Subject,
(
	SELECT sub.subject_units
	FROM subject sub
	WHERE sub.subject_id = grade.subject_subject_id
) Units,
grade_raw_grade Raw_Grade,
equivalence.equivalence_percentile_upper Percentile_Equivalent,
equivalence.equivalence_numerical_grade Numerical_Grade,
equivalence.equivalence_letter_grade Letter_Grade,
equivalence.equivalence_foundation_rating Foundation_Rating,
equivalence.equivalence_school_rating School_Rating,
(
	SELECT sub.subject_approval_status
	FROM subject sub
	WHERE sub.subject_id = grade.subject_subject_id
) Approval_Status
FROM grade
JOIN equivalence on (grade.grade_raw_grade = equivalence.equivalence_numerical_grade OR grade.grade_raw_grade = equivalence.equivalence_letter_grade) AND grade.subject_scholar_school_school_id = equivalence.school_school_id
WHERE 1 = 1
[AND grade.subject_scholar_scholar_id IN ( {scholar_select, VALUE} ) ]
[AND {Scholar_Last_Name} IN ( {scholar_last_name_select, VALUE} ) ]
[AND {Scholar_First_Name} IN ( {scholar_first_name_select, VALUE} ) ]
[AND grade.grade_school_year_start IN ( {school_year_start_select, VALUE})]
[AND grade.grade_school_year_end IN ( {school_year_end_select, VALUE})]
[AND {Term} IN ( {school_term_select, VALUE})]
[AND equivalence.equivalence_foundation_rating IN ({foundation_rating_select, VALUE})]
ORDER BY Scholar_ID</SQLRaw></SQL><QueryColumns><QueryColumn><Name>Scholar_ID</Name><TableName></TableName><ColumnName>subject_scholar_scholar_id</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>Scholar_Last_Name</Name><TableName></TableName><ColumnName>(
