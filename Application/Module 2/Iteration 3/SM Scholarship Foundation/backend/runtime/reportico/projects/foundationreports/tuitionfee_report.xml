<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Tuition Fees Report</ReportTitle><ReportDescription></ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>.DEFAULT</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>.DEFAULT</gridDisplay><gridSortable>.DEFAULT</gridSortable><gridSearchable>.DEFAULT</gridSearchable><gridPageable>.DEFAULT</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT 
scholar_scholar_id Scholar_ID,
(
	SELECT n.scholar_last_name
	FROM scholar n
	WHERE n.scholar_id = tuition.scholar_scholar_id
) Scholar_Last_Name,
(
	SELECT n.scholar_first_name
	FROM scholar n
	WHERE n.scholar_id = tuition.scholar_scholar_id
) Scholar_First_Name,
(
	SELECT n.scholar_middle_name
	FROM scholar n
	WHERE n.scholar_id = tuition.scholar_scholar_id
) Scholar_Middle_Name,
(
	SELECT n.scholar_course
	FROM scholar n
	WHERE n.scholar_id = tuition.scholar_scholar_id
) Scholar_Course,
(
	SELECT n.scholar_year_level
	FROM scholar n
	WHERE n.scholar_id = tuition.scholar_scholar_id
) Scholar_Year_Level,
tuition_school_year_start School_Year_Start,
tuition_school_year_end School_Year_End,
tuition_term Term,
tuition_enrollment_date Enrollment_Date,
(
	SELECT sum(g.subject_units)
	FROM subject g
	WHERE g.subject_term = tuition.tuition_term AND g.scholar_scholar_id = tuition.scholar_scholar_id
) Number_of_Units,
tuition_amount Tuition_Amount,
(
	SELECT n.scholar_sponsor
	FROM scholar n
	WHERE n.scholar_id = tuition.scholar_scholar_id
) Sponsor
FROM tuition
WHERE 1 = 1
[AND {School_Year_Start} IN ( {school_year_start_select} ) ]
[AND {School_Year_End} IN ( {school_year_end_select} ) ]
[AND tuition.tuition_term IN ( {school_term_select} ) ]
ORDER BY Scholar_Last_Name</SQLRaw></SQL><QueryColumns><QueryColumn><Name>Scholar_ID</Name><TableName></TableName><ColumnName>scholar_scholar_id</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>Scholar_Last_Name</Name><TableName></TableName><ColumnName>(
