	FROM scholar n
	WHERE n.scholar_id = deduction.scholar_scholar_id
) Scholar_Last_Name,
(
	SELECT n.scholar_first_name
	FROM scholar n
	WHERE n.scholar_id = deduction.scholar_scholar_id
) Scholar_First_Name,
(
	SELECT n.scholar_middle_name
	FROM scholar n
	WHERE n.scholar_id = deduction.scholar_scholar_id
) Scholar_Middle_Name,
deduction_date Date_Deducted,
deduction_amount Amount,
deduction_remark Remark,
MONTH(deduction.deduction_date) mon
FROM deduction
WHERE 1 = 1
