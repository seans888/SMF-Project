(SELECT s.school_name
FROM schools s
WHERE s.school_id = grades.School_id) School_Name,
(SELECT n.scholar_lastName
FROM scholars n
WHERE n.scholar_id= grades.grade_scholar_id) Scholar_Last_Name,
(SELECT n.scholar_firstName
FROM scholars n
WHERE n.scholar_id= grades.grade_scholar_id) Scholar_First_Name,
(SELECT n.scholar_middleName
FROM scholars n
WHERE n.scholar_id= grades.grade_scholar_id) Scholar_Middle_Name
FROM grades
JOIN equivalence on (grades.grade_value = equivalence.Numerical_Grade OR grades.grade_value = equivalence.Letter_Grade) AND grades.School_id = equivalence.School_id
JOIN schools on (schools.school_id=grades.School_id)
WHERE 1 = 1  
[AND grade_scholar_id IN ( {scholar_select} ) ]  [AND grade_schoolYear IN ( {schoolyear_select} ) ]  [AND grade_Term IN ( {schoolterm_select} ) ]  [AND school_name IN ({school_select})] [AND grade_subject IN ({subject_select})]

