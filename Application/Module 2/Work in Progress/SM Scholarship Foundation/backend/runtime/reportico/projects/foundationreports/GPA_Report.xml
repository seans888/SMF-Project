grade_scholar_id Scholar_ID,
(SELECT n.scholar_lastName
FROM scholars n
WHERE n.scholar_id= grades.grade_scholar_id) Scholar_Last_Name,
(SELECT n.scholar_firstName
FROM scholars n
WHERE n.scholar_id= grades.grade_scholar_id) Scholar_First_Name,
(SELECT s.school_name
FROM schools s
WHERE s.school_id = grades.School_id) School_Name,
grade_SchoolYear School_Year,
grade_Term Term,
grade_subject Subject,
grade_units Units,
grade_value Raw_Grade,
equivalence.Equivalent_upper Percentile_Equivalent,
equivalence.Numerical_Grade,
equivalence.Letter_Grade Letter_Grade,
equivalence.Foundation_Rating Foundation_Rating,
equivalence.School_Rating School_Rating
FROM grades
JOIN equivalence on (grades.grade_value = equivalence.Numerical_Grade OR grades.grade_value = equivalence.Letter_Grade) AND grades.School_id = equivalence.School_id
WHERE 1 = 1
[AND grade_scholar_id IN ( {scholar_select, VALUE} ) ]
[AND grade_SchoolYear IN ( {schoolyear_select, VALUE})]
[AND grade_Term IN ( {schoolterm_select, VALUE})]
[AND equivalence.Foundation_Rating = ({foundationrating_select, VALUE})]
ORDER BY grade_scholar_lastName, grade_schoolYear, grade_Term</SQLRaw></SQL><QueryColumns><QueryColumn><Name>Scholar_ID</Name><TableName></TableName><ColumnName>grade_scholar_id</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>Scholar_Last_Name</Name><TableName></TableName><ColumnName>(SELECT n.scholar_lastName
