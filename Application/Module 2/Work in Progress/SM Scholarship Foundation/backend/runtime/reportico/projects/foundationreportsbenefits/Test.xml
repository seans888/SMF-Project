WHERE benefit_scholar_id=s.scholar_id
) Scholar_lastName,
CASE WHEN r.refund_amount IS NOT NULL THEN r.refund_amount ELSE 0 END Refund_Amount,
CASE WHEN d.deduction_amount IS NOT NULL THEN d.deduction_amount ELSE 0 END Deduction_Amount,
CASE WHEN a.allowance_amount IS NOT NULL THEN a.allowance_amount ELSE 0 END Allowance_Amount,
